<%= form_for @player do |f| %>
<%= f.label :name %>
<%= f.text_field :name %><br>
<%= f.label :birth_date %>
<%= f.date_field :birth_date %><br>
<%= f.label :gender %>
<%= f.text_field :gender %><br>
<%= f.label :start_date %>
<%= f.date_field :start_date %><br>
<%= f.label :email %>
<%= f.email_field :email %><br>
<%= f.label :password %>
<%= f.password_field :password %><br>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %><br>
<%= f.submit "Sign Up"%>
<% end %>
<%= button_to "Log In", {:controller => :sessions,
  :action => 'new' }, :method => :get %>
